{
  "name": "vay-deliver-front",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}",
      "!<rootDir>/src/components/QuantityControl/**"
    ],
    "moduleNameMapper": {
      "^axios$": "axios/dist/node/axios.cjs",
      "^react-router-dom$": "<rootDir>/__mocks__/react-router-dom.tsx"
    }
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^6.10.0",
    "@reduxjs/toolkit": "^2.6.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^17.0.42",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/react-places-autocomplete": "^7.2.14",
    "autoprefixer": "^10.4.7",
    "aws-amplify": "^6.14.1",
    "axios": "^1.9.0",
    "i18next": "^23.6.0",
    "i18next-browser-languagedetector": "^8.1.0",
    "i18next-http-backend": "^3.0.2",
    "lodash": "^4.17.21",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "postcss-preset-env": "^7.7.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-geolocated": "^4.3.0",
    "react-i18next": "^15.5.1",
    "react-places-autocomplete": "^7.3.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.5.3",
    "react-scripts": "^5.0.1",
    "redux-persist": "^6.0.0",
    "redux-persist-indexeddb-storage": "^1.0.4",
    "tailwindcss": "^3.1.2",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx src/",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "fix": "yarn format && yarn lint:fix",
    "amplify:pull": "amplify pull --appId $AMPLIFY_APP_ID --envName $AMPLIFY_ENV_NAME --yes"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@aws-amplify/cli": "^13.0.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/i18next": "^13.0.0",
    "@types/lodash": "^4.17.16",
    "@types/react-i18next": "^8.1.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux-mock-store": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "jest-mock-extended": "^4.0.0-beta1",
    "prettier": "^3.5.3",
    "redux-mock-store": "^1.5.5",
    "ts-jest": "^29.3.2"
  }
}
